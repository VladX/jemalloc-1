/* Defined if __attribute__((...)) syntax is supported. */
#cmakedefine JEMALLOC_HAVE_ATTR

/* Support the experimental API. */
#cmakedefine JEMALLOC_EXPERIMENTAL

/*
 * Define overrides for non-standard allocator-related functions if they are
 * present on the system.
 */
#cmakedefine JEMALLOC_OVERRIDE_MEMALIGN
#cmakedefine JEMALLOC_OVERRIDE_VALLOC

/*
 * At least Linux omits the "const" in:
 *
 *   size_t malloc_usable_size(const void *ptr);
 *
 * Match the operating system's prototype.
 */
#define JEMALLOC_USABLE_SIZE_CONST @JEMALLOC_USABLE_SIZE_CONST@

/* sizeof(void *) == 2^LG_SIZEOF_PTR. */
#define LG_SIZEOF_PTR @LG_SIZEOF_PTR@
